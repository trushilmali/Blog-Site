<%- include("partials/header"); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class= "form-control" name="postTitle" value="" autocomplete="off">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea name="content" class="form-control" rows="8" cols="80"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>
<%- include("partials/footer"); -%>
